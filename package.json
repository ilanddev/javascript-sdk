{
  "name": "iland-sdk",
  "version": "1.0.0",
  "description": "SDK for the ilandcloud REST API",
  "main": "build/iland.js",
  "typings": "build/iland.d.ts",
  "author": "csnyder@iland.com",
  "license": "BSD",
  "repository": " https://github.com/ilanddev/javascript-sdk",
  "scripts": {
    "prepare": "yarn build",
    "info": "npm-scripts-info",
    "lint": "tslint --project tsconfig.json --type-check",
    "prepublish": "yarn build",
    "build": "yarn lint && rm -rf ./build  && webpack --debug && webpack -p && rm -r ./build/build",
    "test": "jest --coverage --maxWorkers 2",
    "docs": "typedoc src/index.ts --excludePrivate --exclude **/__*__/**/* --mode file --theme minimal --out build/docs",
    "publish:docs": "yarn docs && cp circle.yml build/docs && gh-pages -d build/docs"
  },
  "scripts-info": {
    "info": "Display information about the scripts",
    "build": "(Trash and re)build the library",
    "lint": "Lint all typescript source files",
    "test": "Run tests",
    "docs": "Generate HTML API documentation and open it in a browser",
    "publish:docs": "Genrate HTML TypeDoc and deploy to GitHub Pages"
  },
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "@types/jest": "20.0.2",
    "@types/node": "7.0.12",
    "dts-bundle": "^0.7.3",
    "gh-pages": "1.0.0",
    "jest": "20.0.4",
    "ts-jest": "20.0.6",
    "ts-loader": "2.2.2",
    "tslint": "5.5.0",
    "tslint-config-standard": "6.0.1",
    "typedoc": "0.7.1",
    "typescript": "2.4.0",
    "webpack": "3.0.0"
  },
  "keywords": [
    "iland",
    "sdk",
    "typescript"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./test-setup.js",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/.*.test.+(unit|int).ts$",
    "clearMocks": true,
    "bail": true,
    "testPathIgnorePatterns": [
      "/build/",
      "/node_modules/",
      "test-setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/index.ts",
      "!src/sdk/model/json/*.ts"
    ],
    "mapCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test-setup.js",
      "/__test__/.*\\.js$",
      "/build/.*\\.(ts|js)$"
    ]
  },
  "dependencies": {
    "@types/keycloak-js": "2.4.0",
    "axios": "0.16.2",
    "keycloak-js": "1.9.8",
    "rxjs": "5.4.1",
    "tslib": "1.7.1"
  }
}
